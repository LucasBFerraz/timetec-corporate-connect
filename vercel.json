{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://oqtrql4t5vihznitemfxgjnnf40npbua.lambda-url.us-east-1.on.aws/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://timetectelecom.com.br"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
